{
  "name": "hast-util-to-snabbdom",
  "version": "1.0.0",
  "description": "Transform a HAST into a Snabbdom tree.",
  "license": "ISC",
  "keywords": [
    "hast",
    "ast",
    "html",
    "snabbdom",
    "virtual dom"
  ],
  "repository": "syntax-tree/hast-util-to-snabbdom",
  "bugs": "https://github.com/syntax-tree/hast-util-to-snabbdom/issues",
  "author": "Jannis Redmann <mail@jannisr.de>",
  "contributors": [
    "Jannis Redmann <mail@jannisr.de>",
    "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)"
  ],
  "main": "lib/",
  "module": "src/",
  "files": [
    "src",
    "lib"
  ],
  "dependencies": {
    "property-information": "^5.0.0",
    "snabbdom": "^0.7.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-plugin-add-module-exports": "^1.0.0",
    "nyc": "^14.0.0",
    "prettier": "^1.0.0",
    "remark-cli": "^6.0.0",
    "remark-preset-wooorm": "^5.0.0",
    "tape": "^4.0.0",
    "unist-builder": "^1.0.0",
    "xo": "^0.24.0"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "babel src --out-dir lib",
    "format": "remark . -qfo && prettier \"**/*.js\" --write && xo --fix",
    "test-api": "node -r @babel/register test",
    "test-coverage": "nyc --reporter lcov --require @babel/register tape test.js",
    "test": "npm run build && npm run format && npm run test-coverage"
  },
  "browserslist": "> 2.5%, node 6",
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "add-module-exports"
    ]
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "semi": false,
    "trailingComma": "none"
  },
  "xo": {
    "prettier": true,
    "rules": {
      "complexity": "off"
    },
    "ignore": [
      "lib/**/*.js"
    ]
  },
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "functions": 100,
    "branches": 100
  },
  "remarkConfig": {
    "plugins": [
      "preset-wooorm"
    ]
  }
}
